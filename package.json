{
  "name": "configlette",
  "version": "0.3.0",
  "description": "Type-safe configuration from environment variables and .env",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [],
  "repository": "git@github.com:notdaniel/configlette.git",
  "author": "Daniel Keywan Hollister <daniel.hollister@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/notdaniel/configlette",
  "private": false,
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "scripts": {
    "format": "pnpm biome format --write src",
    "build": "pnpm typecheck && pnpm compile",
    "compile": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
